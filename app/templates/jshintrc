{
    // 制限を課すもの
    "camelcase": true,  // 変数名はキャメルケースかアンダースコアの使用を強制
    "curly": true,      // ループブロックと条件ブロックを常に中括弧で囲うことを強制
    "eqeqeq": true,     // == 演算子と != 演算子の使用を禁止
    "indent": 2,        // タブ幅を指定します
    "latedef": true,    // 変数を定義する前に使うのを禁止
    "trailing": true,   // 行末の空白を禁止します
    "unused": true,     //変数が利用されていない場合に警告します
    "undef": true,      //明示的に宣言されていない変数の使用を禁止

    // 制限を緩和するもの
    "debug": true,      // debugger を許可
    "evil": true,       // evalに警告を出さないようにします
    "loopfunc": true,   // ループ内の関数に警告を出さないようにします
    "smarttabs": true  // タブとスペースを混ぜて見た目を整えている場合に警告を出さないようにします

    // 環境を指定するもの
    // "jQuery": true     //jQueryのグローバルオブジェクトを定義済みに
}